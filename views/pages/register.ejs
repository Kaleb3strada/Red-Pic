<script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
<script>
    function checarToken(){
        request={};
        request.token=sessionStorage.getItem('ttoken');
        axios.post("https://dashboard.heroku.com/apps/redpic/logsauth", request)
        .then((response)=>{
            console.log("Conectado al index con éxito");
        })
        .catch((error)=>{
            alert("Sesión expirada");
            window.location.href="/login";
            console.log(error);
        });
    }
    checarToken();
    function logout(){
        request={};
        request.token=(sessionStorage.removeItem('ttoken'));
        window.location.href="/login";
    }
</script>
